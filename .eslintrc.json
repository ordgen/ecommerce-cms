{
  "extends": [
    "airbnb",
    "plugin:meteor/recommended"
  ],
  "rules": {
    "react/jsx-filename-extension": 0,
    "import/no-extraneous-dependencies": 0,
    "react/jsx-boolean-value": 0,
    "react/no-array-index-key": 0,
    "react/forbid-prop-types": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0
	},
	"parser": "babel-eslint",
	"env": {
    "browser": true,
    "node": true
	},
   "plugins": [
    "meteor",
    "react"
  ]
}